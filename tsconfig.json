{
	"extends": "@vue/tsconfig/tsconfig.web.json",
	"include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
	"compilerOptions": {
		"baseUrl": ".",
		"outDir": "dist",
		"types": [
			"node",
			"vite/client",
			"@iconify/types",
			"@types/jsdom",
			"@types/uuid"
		],
		"paths": {
			"@/*": ["./src/*"]
		}
	},
	"references": [
		{
			"path": "./tsconfig.config.json"
		}
	]
}
